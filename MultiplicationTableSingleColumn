/* yuiShirou MMXVII */ 
import java.util.Scanner;
 
class MultiplicationTableSingleColumn{  
	public static void main(String args[]){  
	    int j = 0;
	    int i = 10;
	    Scanner in = new Scanner(System.in);
	    System.out.println(); // newline
	    System.out.println("HELLO! This is a simple multiplication table.");
	   
	    System.out.println(); // newline
	   
		System.out.print("Enter multiplier: ");
        j = in.nextInt(); //get the size of table
            
	    System.out.println("dreaming rainbows... ");
	    System.out.println(); // newline
	    for(int y = 1; y <= i; y++){ //loop the y axis until length
	        int product = y*j; //multiply y axis by x axis position
	        System.out.print( j + " x "+ y +" = "+product); // print out the product of y and j
	        System.out.println(); // newline
	    }
	    System.out.println();
	    System.out.println("results printed.");
	}  
}  
